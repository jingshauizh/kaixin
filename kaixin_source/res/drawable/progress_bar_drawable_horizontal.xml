<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@*android:id/background">
        <shape>
            <corners android:radius="5.0dip" />
            <gradient android:startColor="#ffececec" android:endColor="#ffbbbbbb" android:angle="270.0" android:centerY="0.75" android:centerColor="#ffd3d3d3" />
        </shape>
    </item>
    <item android:id="@*android:id/secondaryProgress">
        <clip>
            <shape>
                <corners android:radius="5.0dip" />
                <gradient android:startColor="#80ffdddd" android:endColor="#a0fabb11" android:angle="270.0" android:centerY="0.75" android:centerColor="#80e89b00" />
            </shape>
        </clip>
    </item>
    <item android:id="@*android:id/progress">
        <clip>
            <shape>
                <corners android:radius="5.0dip" />
                <gradient android:startColor="#ffffdddd" android:endColor="#fffabb11" android:angle="270.0" android:centerY="0.75" android:centerColor="#ffe89b00" />
            </shape>
        </clip>
    </item>
</layer-list>